  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js' --html temp/analyze.html",
    "start": "set BROWSER=none && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint --report-unused-disable-directives src --ext .ts,.tsx",
    "lint:fix": "eslint --report-unused-disable-directives --fix src --ext .ts,.tsx",
    "pull-assets": "gulp copyRaidData",
    "json-schema:lang": "typescript-json-schema ./tsconfig.json ILanguage --required --noExtraProps --out config/lang-schema.json",
    "json-schema:raidextract": "typescript-json-schema ./tsconfig.json IRaidExtractJson --required --out src/process/raidextract-schema.json",
    "json-schema:backup": "typescript-json-schema ./tsconfig.json IBackup --required --noExtraProps --out src/process/backup-schema.json",
    "lang:generate": "node ./scripts/generateBase.js",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -s ./public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
